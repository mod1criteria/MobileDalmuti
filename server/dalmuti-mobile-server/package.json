    "class-validator": "^0.14.0",
    "dalmuti-mobile-root": "file:..",
    "ioredis": "^5.4.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.0",
    "socket.io": "^4.7.5",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "socket.io-client": "^4.7.5",
    "ts-node": "^10.9.2",
